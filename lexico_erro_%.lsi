int add(int a, int b) {
  int result = a + b;
  return result;
}

int subtract(int a, int b) {
  int result = a - b;
  return result;
}

int main() {
  int x = 10;
  int y = 5;
  int sum = add(x, y); 
  int diff = substract(x, y); 
  int product = x % y;
  int quotient = x / 0;
  print(sum, diff, product, quotient);
  return 0;
}
